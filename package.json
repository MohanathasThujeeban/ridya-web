{
      "name": "rideya-saas-monorepo",
      "version": "0.1.0",
      "private": true,
      "workspaces": [
            "apps/*",
            "packages/*"
      ],
      "scripts": {
            "dev": "turbo run dev",
            "build": "turbo run build",
            "test": "turbo run test",
            "lint": "turbo run lint",
            "type-check": "turbo run type-check",
            "clean": "turbo run clean && rm -rf node_modules",
            "format": "prettier --write \"**/*.{ts,tsx,md,json}\""
      },
      "devDependencies": {
            "@types/node": "^20.19.27",
            "@typescript-eslint/eslint-plugin": "^8.51.0",
            "@typescript-eslint/parser": "^8.51.0",
            "eslint": "^8.55.0",
            "prettier": "^3.1.0",
            "turbo": "^2.7.2",
            "typescript": "^5.6.3"
      },
      "engines": {
            "node": ">=20.0.0",
            "pnpm": ">=9.0.0"
      },
      "packageManager": "pnpm@9.0.0"
}
